{
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "jamesclark32/dev-audit": "^1.1",
        "larastan/larastan": "^3.5",
        "laravel/boost": "^1.0",
        "laravel/pint": "^1.22",
        "peckphp/peck": "^0.1.3",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-type-coverage": "^3.5",
        "rector/rector": "^2.0",
        "soloterm/solo": "^0.5.0"
    },
    "scripts": {
        "rector-lint": [
            "./vendor/bin/rector --no-progress-bar $(git diff --name-only --diff-filter=d HEAD | grep '\\.php$')"
        ],
        "rector-lint-check": [
            "./vendor/bin/rector --dry-run --no-progress-bar $(git diff --name-only --diff-filter=d HEAD | grep '\\.php$')"
        ]
    },
}
